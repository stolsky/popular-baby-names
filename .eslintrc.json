{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "comma-dangle": ["error", "never"],
        "indent": ["error", 4],
        "padded-blocks": "off"
    }
}
